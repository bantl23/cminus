/if/         { return IF }
/else/       { return ELSE }
/int/        { return INT }
/void/       { return VOID }
/while/      { return WHILE }
/!=/         { return NEQ }
/==/         { return EQ }
/=/          { return ASSIGN }
/<=/         { return LTE }
/</          { return LT }
/>=/         { return GTE }
/>/          { return GT }
/\+/         { return PLUS }
/-/          { return MINUS }
/\*/         { return TIMES }
/\//         { return OVER }
/\(/         { return LPAREN }
/\)/         { return RPAREN }
/\[/         { return LBRACKET }
/\]/         { return RBRACKET }
/\{/         { return LBRACE }
/\}/         { return RBRACE }
/,/          { return COMMA }
/;/          { return SEMI }
/[0-9]+/     { return NUM }
/[a-zA-Z]+/  { return ID }
/[ \t\n]+/   { /* white space */ }
/./          { return ERROR }
